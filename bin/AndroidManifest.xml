<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phaseii"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:name="com.example.phaseii.Transit"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.phaseii.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.phaseii.LoginNurseActivity" >
        </activity>
        <activity android:name="com.example.phaseii.RegisterPatientActivity" >
        </activity>
        <activity android:name="com.example.phaseii.AddVitalsignActivity" >
        </activity>
        <activity android:name="com.example.phaseii.LoginPhysiciansActivity" >
        </activity>
        <activity android:name="com.example.phaseii.AddPrescription" >
        </activity>
        <activity 
            android:name="com.example.phaseii.UrgencyListActivity" 
            android:label="Hold Down Item to Show More">
        </activity>
        <activity
            android:name="com.example.phaseii.ViewRecordActivity"
            android:label="@string/title_activity_view_record" >
        </activity>
        <activity
            android:name="com.example.phaseii.ViewVitalSignActivity"
            android:label="@string/title_activity_view_vital_sign" >
        </activity>
        <activity
            android:name="com.example.phaseii.ViewPrescriptionActivity"
            android:label="@string/title_activity_view_prescription" >
        </activity>
    </application>

</manifest>
